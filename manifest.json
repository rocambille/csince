{
    "description": "Check since which version a CMake feature exists",
    "manifest_version": 2,
    "name": "CSince",
    "version": "1.0.1",
    "homepage_url": "https://github.com/wasthishelpful/csince",
    "icons": {
        "48": "icons/csince.png"
    },
    "content_scripts": [
        {
            "matches": ["*://cmake.org/cmake/help/*"],
            "js": ["csince.cs.js"]
        }
    ]
}
